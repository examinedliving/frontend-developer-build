<script id="content-blocks" type="text/x-handlebars-template">
{{!-- content blocks templates for content-blocks.html --}}
{{!-- if it is a group with a list, we want to render here --}}
{{#if items}}
<div>
  <h2 class="subsection-heading">{{heading}}</h2>
  {{#dlist items}}{{item}}{{/dlist}}
</div>
{{/if}}
{{!-- ./group --}}
{{!-- if it is an image, we want to render it here --}}
{{#if img}}
<div class="img-wrap">
  <div class="ratio-1-1">
	  <img src="{{imgsrc}}">
  </div>
</div>
{{/if}}
{{!-- ./img --}}
</script>